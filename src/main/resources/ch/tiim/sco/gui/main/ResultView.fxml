<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<SplitPane xmlns:fx="http://javafx.com/fxml/1" fx:id="root" dividerPositions="0.25" maxHeight="1.7976931348623157E308"
           maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity"
           xmlns="http://javafx.com/javafx/8.0.40" fx:controller="ch.tiim.sco.gui.main.ResultView">
    <items>
        <ListView fx:id="swimmers" prefHeight="200.0" prefWidth="200.0"/>
        <VBox>
            <children>
                <TableView fx:id="results" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                    <columns>
                        <TableColumn fx:id="distance" prefWidth="75.0" text="Distance"/>
                        <TableColumn fx:id="stroke" prefWidth="75.0" text="Stroke"/>
                        <TableColumn fx:id="time" prefWidth="75.0" text="Time"/>
                        <TableColumn fx:id="course" prefWidth="75.0" text="Course"/>
                        <TableColumn fx:id="date" prefWidth="75.0" text="Date"/>
                        <TableColumn fx:id="meet" prefWidth="75.0" text="Meet"/>
                        <TableColumn fx:id="reactionTime" prefWidth="75.0" text="Reaction Time"/>
                    </columns>
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                    </columnResizePolicy>
                </TableView>
            </children>
        </VBox>
    </items>
</SplitPane>
