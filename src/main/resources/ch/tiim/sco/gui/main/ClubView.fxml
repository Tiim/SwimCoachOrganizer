<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<SplitPane xmlns:fx="http://javafx.com/fxml/1" fx:id="root" dividerPositions="0.25" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.40" fx:controller="ch.tiim.sco.gui.main.ClubView">
    <items>
        <ListView fx:id="clubs" prefHeight="200.0" prefWidth="200.0" />
      <VBox>
         <children>
              <GridPane VBox.vgrow="ALWAYS">
                  <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  </columnConstraints>
                  <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  </rowConstraints>
                  <children>
                      <Label fx:id="name" text="Label" />
                      <Label fx:id="type" text="Label" GridPane.columnIndex="1" />
                      <Label fx:id="shortName" text="Label" GridPane.rowIndex="1" />
                      <Label fx:id="country" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                      <Label fx:id="englishName" text="Label" GridPane.rowIndex="2" />
                      <Label fx:id="officialCode" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                      <Label fx:id="shortEnglishName" text="Label" GridPane.rowIndex="3" />
                      <Label fx:id="externId" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                      <ListView fx:id="teams" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="200.0" prefWidth="200.0" GridPane.columnSpan="2" GridPane.hgrow="ALWAYS" GridPane.rowIndex="4" GridPane.vgrow="ALWAYS" />
                  </children>
              </GridPane>
                <HBox alignment="CENTER_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity">
                    <children>
                        <Button mnemonicParsing="false" onAction="#onEmail" text="E-Mail"/>
                        <Pane HBox.hgrow="ALWAYS"/>
                        <Button mnemonicParsing="false" onAction="#onEdit" text="Edit" />
                        <Button mnemonicParsing="false" onAction="#onDelete" text="Delete" />
                        <Button mnemonicParsing="false" onAction="#onNew" text="New" />
                    </children>
                </HBox>
         </children>
      </VBox>
    </items>
</SplitPane>
