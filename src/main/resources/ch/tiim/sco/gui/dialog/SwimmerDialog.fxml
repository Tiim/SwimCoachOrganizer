<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" fx:id="root" maxHeight="1.7976931348623157E308"
          maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="295.0"
          prefWidth="455.0" xmlns="http://javafx.com/javafx/8.0.40"
          fx:controller="ch.tiim.sco.gui.dialog.SwimmerDialog">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <TextField fx:id="firstName" promptText="First Name"/>
        <TextField fx:id="email" promptText="E-Mail" GridPane.columnIndex="1"/>
        <TextField fx:id="lastName" promptText="Last Name" GridPane.rowIndex="1"/>
        <TextField fx:id="phonePrivate" promptText="Phone Private" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <TextField fx:id="phoneMobile" promptText="Phone Mobile" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <TextField fx:id="phoneWork" promptText="Phone Work" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
        <TextField fx:id="address1" promptText="Address" GridPane.rowIndex="4"/>
        <TextField fx:id="address2" promptText="City" GridPane.rowIndex="5"/>
        <TextField fx:id="address3" promptText="Country" GridPane.rowIndex="6"/>
        <TextField fx:id="licenseId" promptText="License ID" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="2">
            <children>
                <RadioButton mnemonicParsing="false" text="Male"/>
                <RadioButton fx:id="isFemale" mnemonicParsing="false" text="Female"/>
            </children>
        </HBox>
        <DatePicker fx:id="birthDay" maxWidth="1.7976931348623157E308" promptText="Birthday" GridPane.rowIndex="3"/>
        <TextArea fx:id="notes" prefHeight="200.0" prefWidth="200.0" promptText="Notes" GridPane.columnSpan="2"
                  GridPane.rowIndex="7" GridPane.vgrow="ALWAYS"/>
        <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" GridPane.columnSpan="2"
              GridPane.rowIndex="8">
            <children>
                <Button mnemonicParsing="false" onAction="#onCancel" text="Cancel"/>
                <Button mnemonicParsing="false" onAction="#onSave" text="Save"/>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</GridPane>
