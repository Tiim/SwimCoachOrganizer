<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" fx:id="root" maxHeight="-Infinity" maxWidth="-Infinity"
          minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.40"
          fx:controller="ch.tiim.sco.gui.dialog.ResultDialog">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="183.0" minWidth="10.0" prefWidth="119.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="256.0" minWidth="10.0" prefWidth="256.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Label text="Distance"/>
        <Label text="Stroke" GridPane.rowIndex="1"/>
        <Label text="Time" GridPane.rowIndex="2"/>
        <Label text="Course" GridPane.rowIndex="4"/>
        <Label text="Date" GridPane.rowIndex="5"/>
        <Label text="Meet" GridPane.rowIndex="6"/>
        <Label text="Reaction Time" GridPane.rowIndex="3"/>
        <Spinner fx:id="distance" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1"/>
        <ChoiceBox fx:id="stroke" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <TextField fx:id="time" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <TextField fx:id="reactionTime" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
        <ChoiceBox fx:id="course" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
        <DatePicker fx:id="date" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
        <TextField fx:id="meet" GridPane.columnIndex="1" GridPane.rowIndex="6"/>
        <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" GridPane.columnSpan="2"
              GridPane.rowIndex="7">
            <children>
                <Button mnemonicParsing="false" onAction="#onCancel" text="Cancel"/>
                <Button mnemonicParsing="false" onAction="#onSave" text="Save"/>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</GridPane>
