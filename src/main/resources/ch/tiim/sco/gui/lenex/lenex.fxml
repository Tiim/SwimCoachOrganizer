<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.tiim.sco.gui.lenex.LenexPresenter">
    <top>
        <HBox alignment="CENTER_LEFT">
            <children>
                <Button mnemonicParsing="false" onAction="#onBtnOpen" text="Open" />
            <Button mnemonicParsing="false" text="Close" />
                <ProgressIndicator fx:id="progress" prefHeight="20.0" prefWidth="20.0" visible="false" HBox.hgrow="NEVER" />
                <Label fx:id="path" />
            </children>
        </HBox>
    </top>
    <center>
        <AnchorPane prefWidth="200.0" BorderPane.alignment="CENTER">
            <children>
                <Accordion AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <panes>
                        <TitledPane minHeight="0.0" text="Info">
                            <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0">
                                    <children>
                                        <GridPane layoutX="14.0" layoutY="14.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                            <columnConstraints>
                                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                            </columnConstraints>
                                            <rowConstraints>
                                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                            </rowConstraints>
                                            <children>
                                                <Label text="Application" />
                                                <Label fx:id="lblApp" text="--" GridPane.columnIndex="1" />
                                                <Label text="Name" GridPane.rowIndex="1" />
                                                <Label fx:id="lblName" text="--" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                                                <Label text="Contact" GridPane.rowIndex="2" />
                                                <Label fx:id="lblContact" text="--" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                                            </children>
                                        </GridPane>
                                    </children>
                                </AnchorPane>
                            </content>
                        </TitledPane>
                  <TitledPane text="Imported Results">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <ListView fx:id="resultList" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                           </children>
                        </AnchorPane>
                    </content>
                  </TitledPane>
                    </panes>
                </Accordion>
            </children>
        </AnchorPane>
    </center>
    <right>
        <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
            <children>
                <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onBtnImportMeet" text="Import Meet" />
                <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onBtnImportResults" text="Import Results" />
            </children>
        </VBox>
    </right>
</BorderPane>
